{
  "name": "simplellm",
  "displayName": "simplellm",
  "description": "Extension for interacting with HuggingFace LLMs",
  "repository": "https://github.com/michaelMondoro/simplellm",
  "type": "commonjs",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other",
    "AI",
    "Education",
    "Programming Languages"
  ],
  "activationEvents": ["onLanguage:*"],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
      "simplellm.model_url": {
        "type": "string",
        "default": false,
        "description": "URL of huggingface space hosting your model"
      }
    },
    "commands": [
      {
        "command": "simplellm.showContext",
        "title": "SimpleLLM: showContext"
      },
      {
        "command": "simplellm.ask",
        "title": "SimpleLLM: ask"
      },
      {
        "command": "simplellm.clear",
        "title": "SimpleLLM: clear"
      },
      {
        "command": "simplellm.generate",
        "title": "SimpleLLM: Generate"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
